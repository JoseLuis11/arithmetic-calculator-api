{
  "name": "arithmetic-calculator-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev": "serverless offline --reloadHandler --host localhost --stage local",
    "start": "serverless offline start --stage local",
    "package": "serverless package",
    "deploy:dev": "serverless deploy --stage dev",
    "test:locally": "jest --setupFiles ./tests/setupTests.ts",
    "test": "jest --verbose",
    "eslint": "eslint src/**/*.ts tests/**/*.ts",
    "prepare": "husky install",
    "typeorm": "typeorm-ts-node-commonjs -d src/dataSources/postgres/connection.ts",
    "migrate": "npm run typeorm migration:run",
    "migrate:revert": "npm run typeorm migration:revert",
    "migration:generate": "npm run typeorm migration:generate --pretty=true src/dataSources/postgres/typeorm/migrations/%npm_config_name%"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@serverless/typescript": "^3.29.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.8",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "esbuild": "^0.17.18",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "serverless": "^3.30.1",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.43.1",
    "serverless-offline": "^12.0.4",
    "ts-jest": "^29.1.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.15",
    "ts-node": "^10.9.1"
  }
}
